[loggers]
keys=root

[logger_root]
level=DEBUG
handlers=consoleHandler

[formatters]
keys=consoleFormatter

[formatter_consoleFormatter]
format=%(asctime)s - %(name)s - %(levelname)s - %(message)s
datefmt = '%Y-%m-%d %H:%M'

[handlers]
keys=consoleHandler,fileHandler

[handler_consoleHandler]
class=StreamHandler
formatter=consoleFormatter
level=INFO
args=(sys.stdout,)